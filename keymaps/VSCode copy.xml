<keymap version="1" name="VSCode copy" parent="VSCode">
  <action id="GotoNextBookmark">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="down" />
  </action>
  <action id="GotoPreviousBookmark">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="up" />
  </action>
  <action id="ToggleBookmark">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="k" />
  </action>
</keymap>